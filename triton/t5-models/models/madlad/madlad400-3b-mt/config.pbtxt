name: "madlad400-3b-mt"
backend: "python"
max_batch_size: 8

input [
  {
    name: "INPUT_TEXT"
    data_type: TYPE_STRING
    dims: [ -1 ]
  },
  {
    name: "MAX_LENGTH"
    data_type: TYPE_INT32
    dims: [ 1 ]
    optional: true
  },
  {
    name: "TARGET_LANG"
    data_type: TYPE_STRING
    dims: [ 1 ]
    optional: true
  }
]

output [
  {
    name: "OUTPUT_TEXT"
    data_type: TYPE_STRING
    dims: [ -1 ]
  }
]

instance_group [
  {
    count: 1
    kind: KIND_GPU
  }
]

# Enable dynamic batching for better throughput
dynamic_batching {
  preferred_batch_size: [ 2, 4, 8 ]
  max_queue_delay_microseconds: 100
}